# Fortaleza Basketball Analytics - Production Environment Configuration

# Application
APP_NAME=Fortaleza Basketball Analytics
APP_VERSION=1.0.0
NODE_ENV=production

# API Configuration
API_BASE_URL=https://your-backend-domain.com/api
API_TIMEOUT=30000

# Security
ENABLE_ANALYTICS=false
ENABLE_DEBUG=false

# Performance
ENABLE_GZIP=true
CACHE_STATIC_ASSETS=true
CACHE_DURATION=31536000

# Monitoring
HEALTH_CHECK_ENABLED=true
HEALTH_CHECK_INTERVAL=30

# Logging
LOG_LEVEL=warn
ENABLE_ACCESS_LOGS=true
